###############################################################
# hashCode() - library method (class Object)
# - returns a hash code for an object
# - produces int that is the object's hash code
#   meaning that they are the same physical object
# - parameters:
#   - $s2 - this-pointer (for Object)
#   - ($sp) - old-this-pointer
# - return-value:
#   - $t0 - 1 if the objects were the same; 0 otherwise
###############################################################
996:fcn_hashCode_Object:
    # for now, we'll return the -8 offset word, shifted/added to
    # itself, then XORed with a fixed "random" value
996#    lw $t0,-8($s2) # get word with hash bits from this
1000#    srl $t1,$t0,11 # shift/add to introduce more "randomness"
1004#    addu $t0,$t1

1008#    jr $ra # return, no change to the stack

###############################################################
# equals() - library method (class Object)
# - tests whether two objects are equal
# - produces boolean that tells whether two objects are equal--
#   meaning that they are the same physical object
# - parameters:
#   - $s2 - this-pointer
#   - ($sp) - object to compare to
#   - 4($sp) - old-this-pointer
# - return-value:
#   - $t0 - 1 if the objects were the same; 0 otherwise
###############################################################
1012:fcn_equals_Object:
1012#    move $t0,$s2 # first pointer (this)
1016#    lw $t1,($sp) # second pointer
1020#    seq $t0,$t0,$t1 # produce boolean telling if they are equal
1021##...subu $at,$t0,$t1
1025##...sltiu $t0,$at,1

1028#    jr $ra # return

###############################################################
# hashCode() - library method (class String)
# - returns a hash code for an object
# - produces int that is the object's hash code
#   meaning that they are the same physical object
# - parameters:
#   - $s2 - this-pointer
#   - ($sp) - old-this-pointer
# - return-value:
#   - $t0 - 1 if the objects were the same; 0 otherwise
###############################################################
1032:fcn_hashCode_String:
1032#    move $t0,$s2 # put this pointer in t0

1036#    li $t2,0 # current hash value
1040#    lw $t1,-4($t0) # negative of number of characters in string
1044#    beq $t1,$zero,done_hashCode_String # if zero length => done
1048#    addu $t0,$t1 # address of first character in string +12,
                 # not accounting for padding and header words
1052#    and $t0,0xfffffffc # adjust for padding
1053##...lui $at,65535
1057##...ori $at,$at,0xfffc
1061##...and $t0,$t0,$at
1064#    subu $t1,$t0,$t1 # address at end of string +12
1068:loop_hashCode_String:    
1068#    sll $t4,$t2,5 # multiply by 32
1072#    subu $t2,$t4,$t2 # subtract original, so that it's now multiply by 31
1076#    lbu $t3,-12($t0) # current character
1080#    addu $t2,$t3 # add in current character
1084#    addu $t0,1 # go to next character's address
1088#    bne $t0,$t1,loop_hashCode_String # loop back if more
1092:done_hashCode_String:    

    # push int on stack
1092#    move $t0, $t2 # put $t2 in return slot
1096#    jr $ra # return

###############################################################
# toString() - library method (class Object)
# - returns a String version of the object
#   - $s2 - this-pointer
#   - ($sp) - old-this-pointer
# - return-value:
#   - $t0 - the string version of the object
# ****NOTE: this method counts on the fact that the hashCode method
# ****is in slot 0 of the vtable    
###############################################################
1100:fcn_toString_Object:
    # we need to append:
    # - a string created from the object name in the negative portion of
    #   the vtable
    # - an at-sign "@"
    # - the hex version of the hashcode for the object, with no leading
    #   zeros

    # save return address. Get new this pointer
1100#    subu $sp,4
1104#    sw $ra,($sp)
    
    # get hash value
1108#    lw $t2,-12($s2) # vtable address
1112#    lw $t0,($t2) # method address
1116#    jalr $t0
1120#    subu $sp,8 # push result (int in $t0) on stack
1124#    sw $s5,4($sp)
1128#    sw $t0,($sp)

    # compute print-size of hash value
