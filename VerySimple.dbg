    # allocate the object
388#    jal newObject
        
    ################################################################
    # at this point, the stack looks like (from top):
    # - new object, allocated to be our string
    # - number of characters in the prefix
    # - GC tag (5)
    # - print-size (in bytes) of the hash value
    # - GC tag (5)
    # - hash value
    # - GC tag (5)
    # - return address
    # - old this pointer
    # - ... rest of stack
    # $s7 also contains the address of the new object
    ################################################################

392#    li $t0,CLASS_String # store string vtable address into ...
396#    sw $t0,-12($s7) # ... -12 offset in object header
400#    lw $t2,-12($s2) # our object's vtable address
