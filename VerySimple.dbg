180:fcn_hashCode_String:
180#    move $t0,$s2 # put this pointer in t0

184#    li $t2,0 # current hash value
188#    lw $t1,-4($t0) # negative of number of characters in string
192#    beq $t1,$zero,done_hashCode_String # if zero length => done
196#    addu $t0,$t1 # address of first character in string +12,
                 # not accounting for padding and header words
200#    and $t0,0xfffffffc # adjust for padding
201##...lui $at,65535
205##...ori $at,$at,0xfffc
209##...and $t0,$t0,$at
212#    subu $t1,$t0,$t1 # address at end of string +12
216:loop_hashCode_String:    
216#    sll $t4,$t2,5 # multiply by 32
220#    subu $t2,$t4,$t2 # subtract original, so that it's now multiply by 31
224#    lbu $t3,-12($t0) # current character
228#    addu $t2,$t3 # add in current character
232#    addu $t0,1 # go to next character's address
236#    bne $t0,$t1,loop_hashCode_String # loop back if more
240:done_hashCode_String:    
