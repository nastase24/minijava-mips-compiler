# instanceOf (millicode) - tests whether an object is
#   a member of a given class (or subclass)
#  - returns false if object is null
#  - parameters:
#    - ($sp) object to check
#    - $t0 address of vtable for target-class
#    - $t1 address one past vtable address of last
#      subclass of target-class
#  - return value:
#    - ($sp) true (1) or false (0), depending on whether
#      object is a member
#  - side-effects: clobbers $t2 and $t3
########################################################
3120:instanceOf:
    # get object, go return false if null
3120#    lw $t2,($sp)
3124#    beq $t2,$zero,doneInstanceOf
    
    # get vtable address of object, determine if we're >= the
    # lower limit, and if we're < the higher limit
3128#    lw $t2,-12($t2) # vtable address
3132#    sge $t0,$t2,$t0 # are we >= the lower limit?
3133##...slt $t0,$t2,$t0
3137##...xori $t0,$t0,0x1
3140#    slt $t1,$t2,$t1 # are we < the higher limit?
    
    # store the AND of the two conditions onto the stack; return
3144#    and $t2,$t0,$t1
3148#    sw $t2,($sp)
3152:doneInstanceOf: # if we reach here via branch, stack-top is zero,
            # which will now represent false
3152#    jr $ra


###########################################################
# jump-targets to terminate program:
# - exit: returns normally
# - outOfHeapMemory: prints "out of memory" error and returns
###########################################################

3156:exitError:
    # assumes $a0 has pointer to null-terminated string

    # print the string
3156#    li $v0,4 # syscall-code for print-string
3160#    syscall
3164:exit:
    # print termination message
3164#    li $v0,4 # syscall-code for print-string
3168#    la $a0,exitString
3172#    syscall

    # print number of garbage collections
3176#    li $v0,4 # syscall-code for print-string
3180#    la $a0,gcCountMsg
3184#    syscall # print "Number of ..."
3188#    lw $a0,gcCount # number of GC's
3192#    li $v0,1 # code for print-int
3196#    syscall
3200#    li $v0,4 # syscall-code for print-string
3204#    la $a0,newlineString
3208#    syscall # print newline

    # terminate execution
3212#    li $v0,10 #syscall-code for "exit"
3216#    syscall

