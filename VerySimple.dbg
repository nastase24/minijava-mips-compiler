345##...and $t1,$t1,$at
348#    beq $t1,$zero,toString_checkTagBit
352#    subu $t1,$t2,$t0 # size of the prefix string, in bytes
356#    subu $sp,8
360#    sw $t1,($sp)
364#    sw $s5,4($sp) # push size onto stack

    # determine the header words for the string object's header
368#    move $s7,$zero # -4 offset header word
372#    lw $t4,8($sp) # bytes for hash value
376#    addu $t4,$t1 # bytes for prefix
    # add 8, to account for:
