
    # save return address. Get new this pointer
248#    subu $sp,4
252#    sw $ra,($sp)
    
    # get hash value
256#    lw $t2,-12($s2) # vtable address
260#    lw $t0,($t2) # method address
264#    jalr $t0
268#    subu $sp,8 # push result (int in $t0) on stack
272#    sw $s5,4($sp)
276#    sw $t0,($sp)

    # compute print-size of hash value
280#    li $t0, 1
284#    lw $t1, ($sp)
