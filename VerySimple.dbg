###############################################################
# equals() - library method (class String)
# - the String version of the .equals method
# - produces false if the second object is null or is not a String ;
#   otherwise produces true iff the two strings have the same contents
# - parameters:
#   - $s2 - this-pointer
#   - ($sp) - object to compare to
#   - 4($sp) - old-this-pointer
# - return-value:
#   - ($sp) - 1 if the objects were the same; 0 otherwise
###############################################################
568:fcn_equals_String:
    
    # quick test for obvious false
568#    lw $t0,($sp) # second parameter
572#    beq $t0,$zero,goEsFalse # go return false if null
576#    lw $t1,-12($t0) # vtable pointer
580#    la $t2,CLASS_String # string vtable pointer
584#    bne $t1,$t2,goEsFalse # go return false if not a string

    # save $ra, $s2, second parameter
    # we don't need to swap $s2, 
    # because it's still in the string class.
588#    subu $sp,12
592#    sw $ra,8($sp)
596#    sw $s2,4($sp)
600#    sw $t0,($sp)
604#    jal fcn_compareTo_String
608#    lw $s2, 4($sp) #restore this pointer and pop parameters
612#    addu $sp, 8 # pop parameters

    # return value is 1 iff result is 0; else 0
616#    seq $t0,$t0,$zero
617##...subu $at,$t0,$zero
621##...sltiu $t0,$at,1

    # pop return address and return $t0
624#    lw $ra,($sp)
628#    addu $sp,4

632#    jr $ra # return
    
636:goEsFalse:
    # put 0 in $t0 and return
636#    li $t0, 0
640#    jr $ra # return
